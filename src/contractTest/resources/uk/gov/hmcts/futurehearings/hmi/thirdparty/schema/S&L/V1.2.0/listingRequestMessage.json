{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://www.gov.uk/listingRequest.json",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"listingRequest": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"meta": {
					"$ref": "reusable.json#/definitions/meta"
				},
				"_case": {
					"$ref": "reusable.json#/definitions/_case"
				},
				"entities": {
          "type": "array",
          "items": {
            "$ref": "reusable.json#/definitions/entity"
          }
        },
				"listing": {
				  "type": "object",
				  "additionalProperties": false,
          "properties": {
            "sessionIdCaseHQ": {
              "type": "string",
              "pattern": "^[0-9]*$"
            },
            "listingType": {
              "type": "string"
            },
            "listingStartTime": {
              "type": "string",
              "format": "time"
            },
            "listingDuration": {
              "type": "integer",
              "exclusiveMinimum": 0
            }
          },
          "required": [
            "listingType",
            "listingStartTime",
            "listingDuration"
          ]
				}
			},
			"required": [
				"meta",
				"_case",
				"listing"
			]
		}
	},
	"required": [
		"listingRequest"
	]
}